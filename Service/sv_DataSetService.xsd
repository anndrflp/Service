                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT
A.HANDLE,
A.SERVICO,
A.DATAINICIAL INICIO,
A.DATAFINAL FINAL,
A.DURACAO DURACAO,
A.OBS OBSERVACAO,
B.NOME CLIENTE,
B.CONTATO ,
C.CIDADE,
C.BAIRRO,
F.REFERENCIA,
C.num NUMERO,
C.RUA,
D.NOME EQUIPE,
E.NOME SITUACAO

FROM SV_SERVICO A
INNER JOIN SV_CLIENTE B ON A.CLIENTE = B.HANDLE
INNER JOIN SV_ENDERECO C ON B.ENDERECO = C.HANDLE
INNER JOIN SV_EQUIPE D ON A.EQUIPE = D.HANDLE
INNER JOIN SV_STATUS E ON A.STATUS = E.HANDLE
INNER JOIN SV_REFERENCIA F ON C.REFERENCIA = F.HANDLE


</CommandText>
